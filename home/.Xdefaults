! THE COMMENT CHAR
! Based on https://github.com/ramnes/dotfiles
URxvt.termName: xterm-256color

! System-wide extentions path: /usr/lib64/urxvt/perl
! You can make URLs in the terminal clickable using the matcher extension
URxvt.perl-ext-common: default,matcher,selection-to-clipboard
URxvt.matcher.button: 1

URxvt.saveLines: 99999
URxvt.scrollBar: false

! `fc-list` to list fonts
URxvt.font: xft:DejaVu Sans Mono:pixelsize=20, xft:WenQuanYi Micro Hei Mono:size=16
URxvt.boldFont: xft:DejaVu Sans Mono:style=Bold:pixelsize=20
URxvt.letterSpace: -1

URxvt.depth: 32
! URxvt.foreground: #FFFFFF
! URxvt.background: [90]#1a1a1a

! URxvt.cursorBlink: true
! URxvt.cursorColor: #888888
! URxvt.cursorColor2: #FFFFFF

! dark black red green yellow blue magenta cyan white
! URxvt.color0: #2e3436
! URxvt.color1: #cc0000
! URxvt.color2: #4e9a06
! URxvt.color3: #c4a000
! URxvt.color4: #3465a4
! URxvt.color5: #75507b
! URxvt.color6: #06989a
! URxvt.color7: #d3d7cf

! light black red green yellow blue magenta cyan white
! URxvt.color8: #555753
! URxvt.color9: #ef2929
! URxvt.color10: #8ae234
! URxvt.color11: #fce94f
! URxvt.color12: #729fcf
! URxvt.color13: #ad7fa8
! URxvt.color14: #34e2e2
! URxvt.color15: #ffffff

Xft.antialias: true
Xft.dpi: 96
Xft.hinting: true
Xft.hintstyle: hintslight
Xft.rgba: none
